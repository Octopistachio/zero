if !ini_key_exists('sound','music_muted')
    ini_write_real('sound','music_muted',false)

if (ini_read_real('sound','music_muted',false) == false){
    audio_play_sound(aGameMusic,1,true);
    global.muteMusic = false;
}
else{
    audio_stop_sound(aGameMusic);
    global.muteMusic = true;
}

if !ini_key_exists('sound','muted')
    ini_write_real('sound','muted',false)

if (ini_read_real('sound','muted',false) == false)
    global.mute = false;
else
    global.mute = true;
    
ini_close();
    
